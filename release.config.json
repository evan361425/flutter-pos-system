{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["enhancement"]
    },
    {
      "title": "## 🐛 Bugs",
      "labels": ["bug"]
    }
  ],
  "ignore_labels": ["ignore"],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n## 👻 Others\n\n${{UNCATEGORIZED}}\n",
  "pr_template": "- ${{TITLE}} #${{NUMBER}} : ${{AUTHOR}}",
  "empty_template": "Caught some bugs, time to clean up!",
  "max_tags_to_fetch": 10,
  "max_pull_requests": 20,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": ["master"]
}
