{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["enhancement"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["bug"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n## 👻 其他\n\n${{UNCATEGORIZED}}\n",
  "pr_template": "- ${{TITLE}} #${{NUMBER}} : ${{AUTHOR}}",
  "empty_template": "沒找到相關的 PR，可能需要查找 DIFF",
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "tag_resolver": {
    "method": "semver"
  },
  "base_branches": [
    "master"
  ]
}
